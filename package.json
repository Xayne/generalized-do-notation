{
    "name": "generalized-do-notation",
    "version": "1.0.0",
    "description": "util for building type-safe do notation for any monad in typescript",
    "keywords": [
        "fp",
        "functional-programming",
        "typescript",
        "do-notation"
    ],
    "author": "xayne <xayne.rpc@gmail.com>",
    "license": "MIT",
    "sideEffects": false,
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "rimraf ./lib && tsc --project tsconfig.build.json",
        "run-examples": "rimraf ./examples-compiled && tsc --project tsconfig.examples.json && node examples-compiled/examples/entry.js",
        "prepare": "npm run build"
    },
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Xayne/generalized-do-notation.git"
    },
    "bugs": {
        "url": "https://github.com/Xayne/generalized-do-notation/issues"
    },
    "homepage": "https://github.com/Xayne/generalized-do-notation#readme",

    "dependencies": {
        "fp-essentials": "^0.1.1"
    },

    "devDependencies": {
        "rimraf": "^3.0.2"
    }
}
